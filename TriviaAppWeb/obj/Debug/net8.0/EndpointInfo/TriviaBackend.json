{
  "openapi": "3.0.1",
  "info": {
    "title": "TriviaBackend",
    "version": "1.0"
  },
  "paths": {
    "/api/trivia/categories": {
      "get": {
        "tags": [
          "Trivia"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/trivia/questions": {
      "post": {
        "tags": [
          "Trivia"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TriviaConfig"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TriviaConfig"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TriviaConfig"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/health": {
      "get": {
        "tags": [
          "TriviaBackend"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TriviaConfig": {
        "type": "object",
        "properties": {
          "category": {
            "type": "string",
            "nullable": true
          },
          "difficulty": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}